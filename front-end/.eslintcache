ebar\">\n                    <Sidebar\n                        currentPage={endPoint}\n                        onFeedChange={handleFeedChange}\n                        onHome={handleHome}\n                        onFavourite={handleFavourite}\n                        onMemoryLane={handleMemoryLane}\n                        onSignout={props.onSignOut}\n                    />\n                </div>\n                <div className=\"feed\">\n                    {\n                        imgUrls.length === 0\n                        ?\n                        (\n                            endPoint === 'home'\n                            ?\n                            <p className=\"no-results\">Upload your first memory!</p>\n                            :\n                            (\n                                endPoint === 'favourites'\n                                ?\n                                <p className=\"no-results\">You don't have any favourite memories yet.</p>\n                                :\n                                <p className=\"no-results\">No memories matched your search.</p>\n                            )\n                        )\n                        :\n                        imgUrls.map((imgUrl, index) => {\n                            return (\n                                <LazyLoad key={index} placeholder=\"...\" height={200} offset={100}>\n                                    <Feed imgUrl={imgUrl} key={index} onFeedChange={handleFeedChange} onSearch={handleSearch} />\n                                </LazyLoad>\n                            );\n                        })\n                    }\n                </div>\n            </div>\n        </div>\n    );\n}\n\nexport default Home;","/Users/nikhil.ismail/Github/flashback2.0/front-end/src/components/Navigation/Navigation.js",["63","64"],"import React, { useState } from 'react';\nimport axios from 'axios';\nimport Modal from '../Modal/Modal';\nimport Upload from '